{
  "name": "fariv/pulse",
  "description": "Minimal OpenSwoole microframework for PHP 8.5+",
  "type": "library",
  "require": {
    "php": ">=8.4"
  },
  "autoload": {
    "psr-4": {
      "Pulse\\": "src/",
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pulse\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --bootstrap vendor/autoload.php tests"
  },
  "license": "MIT",
  "minimum-stability": "stable"
}
